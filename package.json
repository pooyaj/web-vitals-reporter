{
  "name": "web-vitals-reporter",
  "version": "0.1.0",
  "description": "A tiny (800 B) utility to simplify web vitals reporting",
  "repository": "treosh/web-vitals-reporter",
  "bugs": "https://github.com/treosh/web-vitals-reporter/issues",
  "license": "MIT",
  "sideEffects": false,
  "types": "types/index.d.js",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "prettier -c src README.md && tsc -p ."
  },
  "dependencies": {
    "web-vitals": "^0.2.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.5.0",
    "prettier": "^2.0.5",
    "size-limit": "^4.5.0",
    "typescript": "^3.9.5"
  },
  "keywords": [
    "web vitals",
    "api",
    "metrics",
    "FCP",
    "LCP",
    "FID",
    "CLS",
    "CrUX",
    "chrome user experience report",
    "web performance",
    "time to first byte",
    "first contentful paint",
    "largest contentful paint",
    "first input delay",
    "cumulative layout shift",
    "network connection",
    "device memory"
  ],
  "size-limit": [
    {
      "limit": "800B",
      "path": "./src/index.js"
    }
  ]
}
